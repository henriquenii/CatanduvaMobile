    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.NoActionBar">
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivitySobre"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityCompras"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityTransportes"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityHospedagem"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityAlimentacao"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityEntretenimento"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityEsporteLazer"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityUtilidadePublica"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivitySaude"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityMidia"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityOnibusUrbano"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".ActivityMaps"
            android:label="@string/title_activity_maps" />
        <activity
            android:name=".ActivityOnibusUrbanoTerminalUrbano"
            android:screenOrientation="portrait" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
