            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".ActivityHistoriaDaCidade"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityOnibusUrbanoRotas"
            android:label="@string/title_activity_onibus_urbano_rotas"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity
            android:name=".ActivityFinanceiras"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityOnibusUrbanoRotasAmendolaAgudoromaoClubedecampo"
